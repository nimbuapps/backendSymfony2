<!DOCTYPE html>
<html>
    <head>
        {% block meta%}
            <meta http-equiv="Content-Type" content="text/html" charset=utf-8" />
        {% endblock%}
        <title>{% block title %}Nimbu Notificaciones Push{% endblock %}</title>
        <!--[if lt IE 9]>
            <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
        {% block stylesheets %}
            <link href="{{ asset('css/bootstrap.css') }}" type="text/css" rel="stylesheet" />          
            <link href="{{ asset('css/custom.css') }}" type="text/css" rel="stylesheet" />            
            <link href="{{ asset('css/bootstrap-select.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('css/bootstrap-select.min.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('css/docs.min.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('css/font-awesome.css') }}" type="text/css" rel="stylesheet" />


        {% endblock %}
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body {% block estilo_body %}{% endblock %}>
        <div class="container">
            {% block body %}
                <div class="container">
                    Esto es el body                
                </div>
            {% endblock %}               
        </div>
        {% block footer %}
            Estos es el footer
        {% endblock %}
        {% block javascripts %}
            <script src="{{ asset('js/jquery-1.11.1.js') }}"></script>
            <script src="{{ asset('js/ajax.js') }}"></script>
            <script src="{{ asset('js/bootstrap-select.min.js') }}"></script>            
            <script src="{{ asset('js/jquery-1.11.1.min.js') }}"></script>
            <script src="{{ asset('js/bootstrap.min.js') }}"></script>      
            <script src="{{ asset('js/bootstrap-select.js') }}"></script>
            <script src="{{ asset('js/maskedinput.js') }}"></script>
            <script src="{{ asset('js/custom.js') }}"></script>
            <script type="text/javascript" src="{{ asset('js/alertify/alertify.js') }}"></script>
            <link rel="stylesheet" href="{{ asset('js/alertify/alertify.core.css') }}" />
            <link rel="stylesheet" href="{{ asset('js/alertify/alertify.default.css') }}" />
            <!-- Bootstrap 3 theme -->
            <link rel="stylesheet" href="{{ asset('js/alertify/alertify.bootstrap3.css') }}" />
          
        {% endblock %}
        {% block javascriptsAdd %}

        {% endblock %}
    </body>
</html>
