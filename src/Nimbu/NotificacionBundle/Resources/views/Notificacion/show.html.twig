{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Notificacion</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Mensaje</th>
                <td>{{ entity.mensaje }}</td>
            </tr>
            <tr>
                <th>Fechahora</th>
                <td>{{ entity.fechaHora|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Validez</th>
                <td>{{ entity.validez }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('notificacion') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('notificacion_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
